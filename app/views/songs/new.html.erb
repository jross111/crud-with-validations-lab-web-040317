<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>New Song!</title>
  </head>
  <body>
    <h2>New Song Plz!</h2>
    <%= form_for @song do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :released %>
      <%= f.check_box :released %>
      <%= f.label :release_year %>
      <%= f.number_field :release_year %>
      <%= f.label :artist_name %>
      <%= f.text_field :artist_name %>
      <%= f.label :genre %>
      <%= f.text_field :genre %>
      <%= f.submit %>

    <% end %>

  </body>
</html>
